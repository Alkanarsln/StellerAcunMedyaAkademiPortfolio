    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3>Yeni Hakkımızda Ekleme Sayfası</h3>
<br />

<form  method="post">
    <input type="text" name="ImageUrl" class="form-control" placeholder="Resim Url Giriniz" />
    <br />
